{
  "name": "EZRA-XD",
  "description": "Ezra Multi Device whatsappbot",
  "logo": "https://i.hizliresim.com/6Bbj7i.jpg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/Louis-X0/EZRA-XD",
  "website": "https://github.com/Louis-X0/EZRA-XD",
  "success_url": "https://github.com/Louis-X0/EZRA-XD",
  "stack": "container",
  "env": {
    "ANTI_LINK": {
      "description": "true/false",
      "required": false
    },
    "ANTILINK_ACTION": {
      "description": "kick/warn",
      "required": false
    },
    "RMBG_KEY": {
      "description": "Remove bg api key ",
      "required": false
    },
    "SESSION_ID": {
      "description": "Ezra-Xd Session ID",
      "required": true
    },
    "HANDLERS": {
      "description": "prefix for commands, enter false for no prefix",
      "required": true,
      "value": "."
    },
    "LOGS": {
      "description": "need to show message logs ?",
      "required": false
    },
    "PACKNAME": {
      "description": "Sticker Packname",
      "required": false
      "value": "𝐄𝐳𝐫𝐚-𝐗𝐃"
    },

    "GOODBYE_MSG": {
      "description": "goodbye Message",
      "required": false
    },
    "AUTHOR": {
      "description": "Sticker Pack AUTHOR",
      "required": false
      "value": "©𝐋𝐨𝐮𝐢𝐬-𝐗𝟎"
      
    },
    "SUDO": {
      "description": "sudo numbers with country code seperate with , for multiple numbers",
      "required": true
       "value": "48888887897"
      
    },
    "WORK_TYPE": {
      "description": "work type",
      "required": "true",
      "value": "private"
     },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
     },
    "BOT_INFO": {
      "description": "bot info",
      "required": "true",
      "value": "𝐄𝐳𝐫𝐚-𝐗𝐃;𝐋𝐨𝐮𝐢𝐬-𝐗𝟎;photourl"
    }
  }
}
